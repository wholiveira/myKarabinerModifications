{
    "title": "Tab to Mission Control",
    "rules": [
      {
        "description": "Tab to Mission Control",
        "manipulators": [
            {
                "type": "basic",
                "from": {
                    "key_code": "tab"
                },
                "to_after_key_up": [
                    {
                        "key_code": "tab"
                    }
                ],
                "to_if_held_down": [
                    {
                        "key_code": "mission_control2",
                        "halt": true
                    }
                ]
            }            
        ]
      }
    ]
  }